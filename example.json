{
	"$schema": "https://turnoffthetv.xyz/jpl.json",
	"title": "Example page",
	"styles": ["./style.css"],
	"scripts": ["./script.js"],
	"elements": [
		{
			"type": "header",
			"level": 1,
			"content": "Example page"
		},
		{
			"type": "paragraph",
			"content": "This is a page to demo JPL.\nHere are some things it can do:"
		},
		{
			"type": "list",
			"list": "unordered",
			"items": [
				{
					"content": "Headers"
				},
				{
					"content": "Paragraphs"
				},
				{
					"content": "Lists (like this one)"
				},
				{
					"type": "link",
					"href": "https://turnoffthetv.xyz",
					"content": "Lists can have elements, too!"
				}
			]
		},
		{
			"type": "link",
			"href": "https://turnoffthetv.xyz",
			"content": "There are links, too!"
		},
		{
			"type": "paragraph",
			"content": "This is styled inline",
			"style": "color: lightgreen;"
		},
		{
			"type": "paragraph",
			"content": "This is styled using a class",
			"class": "red"
		},
		{
			"type": "paragraph",
			"content": "This is also styled using a class. Two of them!",
			"class": ["red", "black"]
		},
		{
			"type": "paragraph",
			"content": "This is styled both inline and with a class",
			"style": "color: lightgreen;",
			"class": "black"
		},
		{
			"type": "paragraph",
			"content": "This is to be edited by a script",
			"id": "editMe"
		}
	]
}